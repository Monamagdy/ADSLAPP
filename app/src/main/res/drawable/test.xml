<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="104dp"
        android:left="40dp"
        android:right="40dp"
        android:top="-10dp">
        <shape android:shape="oval">
            <size
                android:width="20dp"
                android:height="20dp" />
            <solid android:color="@color/grey_9B9B9B" />
        </shape>
    </item>
    <item>
        <rotate
            android:fromDegrees="90"
            android:toDegrees="90">
            <shape android:shape="line">

                <stroke
                    android:width="0.88dp"
                    android:color="@color/grey_979797"
                    android:dashGap="7px"
                    android:dashWidth="45px" />
            </shape>


        </rotate>
    </item>


</layer-list>

